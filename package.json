{
  "name": "vstoys",
  "displayName": "VsToys",
  "description": "VsCode Toys is a collection of utilities for Visual Studio Code.",
  "version": "0.0.6",
  "publisher": "Logonz",
  "icon": "VsCodeToys-icon-512x512.png",
  "license": "MIT",
  "engines": {
    "vscode": "^1.89.1"
  },
  "tags": [
    "dot-repeat",
    "motion"
  ],
  "categories": [
    "Other",
    "Keymaps"
  ],
  "main": "./dist/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Logonz/vscode-toys.git"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vstoys.debug.showQuickPick",
        "title": "VSCode Toys: Show Debug Menu"
      },
      {
        "command": "vstoys.smart-open.showQuickPick",
        "title": "Smart Open: Search Files",
        "category": "VsToys"
      },
      {
        "command": "vstoys.smart-open.navigateUp",
        "category": "VsToys",
        "title": "Smart Open: Navigate Up",
        "enablement": "vstoys.smart-open.searching"
      },
      {
        "command": "vstoys.smart-open.navigateDown",
        "category": "VsToys",
        "title": "Smart Open: Navigate Down",
        "enablement": "vstoys.smart-open.searching"
      },
      {
        "command": "vstoys.smart-open.selectFile",
        "category": "VsToys",
        "title": "Smart Open: Select File",
        "enablement": "vstoys.smart-open.searching"
      },
      {
        "command": "vstoys.smart-open.deleteChar",
        "category": "VsToys",
        "title": "Smart Open: Delete Character",
        "enablement": "vstoys.smart-open.searching"
      },
      {
        "command": "vstoys.semantic-jump.jump",
        "category": "VsToys",
        "title": "Semantic Jump: Start Jump Mode"
      },
      {
        "command": "vstoys.semantic-jump.debug",
        "category": "VsToys",
        "title": "Semantic Jump: Debug Mode (Show All Tokens)"
      },
      {
        "command": "vstoys.registers.showRegisterPastePicker",
        "category": "VsToys",
        "title": "Registers: Show Register Paste Picker"
      },
      {
        "command": "vstoys.registers.showRegisterCopyPicker",
        "category": "VsToys",
        "title": "Registers: Show Register Copy Picker"
      },
      {
        "command": "vstoys.registers.clearAllRegisters",
        "category": "VsToys",
        "title": "Registers: Clear All Registers"
      },
      {
        "command": "vstoys.paste-replace.clipboardPasteReplace",
        "category": "VsToys",
        "title": "Paste Replace"
      },
      {
        "command": "vstoys.paste-replace.clipboardPasteSmart",
        "category": "VsToys",
        "title": "Paste Smart"
      },
      {
        "command": "vstoys.motions.start",
        "category": "VsToys",
        "title": "Motions: Start Interactive"
      },
      {
        "command": "vstoys.motions.di",
        "category": "VsToys",
        "title": "Motions: Delete Inside"
      },
      {
        "command": "vstoys.motions.da",
        "category": "VsToys",
        "title": "Motions: Delete Around"
      },
      {
        "command": "vstoys.motions.yi",
        "category": "VsToys",
        "title": "Motions: Yank Inside"
      },
      {
        "command": "vstoys.motions.ya",
        "category": "VsToys",
        "title": "Motions: Yank Around"
      },
      {
        "command": "vstoys.motions.vi",
        "category": "VsToys",
        "title": "Motions: Select Inside"
      },
      {
        "command": "vstoys.motions.va",
        "category": "VsToys",
        "title": "Motions: Select Around"
      },
      {
        "command": "vstoys.motions.escape",
        "category": "VsToys",
        "title": "Motions: Escape/Cancel"
      },
      {
        "command": "vstoys.hyper.deactivateAll",
        "category": "VsToys",
        "title": "Hyper: Deactivate All"
      },
      {
        "command": "vstoys.goto-line.goto",
        "category": "VsToys",
        "title": "VsToys - Goto Line: Go to Line"
      },
      {
        "command": "vstoys.goto-line.goto-relative",
        "category": "VsToys",
        "title": "VsToys - Goto Line: Go to Relative Line"
      },
      {
        "command": "vstoys.git.fileDecorator.refresh",
        "category": "VsToys",
        "title": "File Decorator: Refresh Decorations - (VsToys)"
      },
      {
        "command": "vstoys.git.stageFile",
        "category": "VsToys",
        "title": "Git: Stage File - (VsToys)"
      },
      {
        "command": "vstoys.git.stageHunk",
        "category": "VsToys",
        "title": "Git: Stage Hunk at Selection - (VsToys)"
      },
      {
        "command": "vstoys.copy-highlight.copy",
        "category": "VsToys",
        "title": "Copy Highlight: Copy"
      },
      {
        "command": "vstoys.clear-line.clearLines",
        "category": "VsToys",
        "title": "Clear Line: Clear Lines"
      }
    ],
    "configuration": {
      "title": "VsToys",
      "properties": {
        "vstoys.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Global enable/disable for the VSCode Toys",
          "order": 100
        },
        "vstoys.smart-open.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable the Smart Open module in VSCode Toys"
        },
        "vstoys.smart-open.debugDetails": {
          "type": "boolean",
          "default": false,
          "description": "Enable detailed debugging information for the Smart Open module"
        },
        "vstoys.smart-open.maxWorkspaceFiles": {
          "type": "number",
          "default": 5000,
          "description": "Maximum number of files to index in the workspace, tip is to use gitignore to exclude files"
        },
        "vstoys.semantic-jump.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable the Semantic Jump module in VSCode Toys",
          "order": 8000
        },
        "vstoys.semantic-jump.jumpCharacters": {
          "type": "string",
          "default": "fjdkslaghrueiwoncmvFJDKSLAGHRUEIWONCMV",
          "description": "Characters used for jumping to semantic tokens. Auto-sliced: first 8 chars = home row (single chars for high-priority targets), next 12 chars = extended (used in sequences), remaining = fallback alternatives for conflict avoidance",
          "order": 8001
        },
        "vstoys.semantic-jump.includedTokenTypes": {
          "type": "array",
          "default": [
            "function",
            "method",
            "class",
            "interface",
            "type",
            "enum",
            "enumMember",
            "variable",
            "property",
            "parameter",
            "namespace",
            "typeParameter",
            "struct",
            "decorator",
            "event",
            "macro",
            "label"
          ],
          "description": "Semantic token types to include in jump targets",
          "items": {
            "type": "string"
          },
          "order": 8002
        },
        "vstoys.semantic-jump.includeAllTokenTypes": {
          "type": "boolean",
          "default": false,
          "description": "Include ALL semantic token types (overrides includedTokenTypes when true)",
          "order": 8003
        },
        "vstoys.semantic-jump.mode": {
          "type": "string",
          "enum": [
            "simple",
            "adaptive",
            "progressive"
          ],
          "default": "adaptive",
          "description": "Jump mode: simple (original), adaptive (Flash.nvim-style density), progressive (always two-char)",
          "enumDescriptions": [
            "Original simple mode with fixed character assignment",
            "Adaptive mode that adjusts to target density (recommended)",
            "Progressive mode that always uses two-character sequences"
          ],
          "order": 8004
        },
        "vstoys.semantic-jump.clusteringEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Group related tokens (e.g., property chains) to reduce clutter",
          "order": 8005
        },
        "vstoys.semantic-jump.tokenPriorities": {
          "type": "object",
          "default": {
            "function": 100,
            "method": 100,
            "class": 90,
            "interface": 85,
            "type": 80,
            "enum": 75,
            "decorator": 70,
            "namespace": 65,
            "variable": 50,
            "property": 40,
            "parameter": 30,
            "enumMember": 25,
            "event": 20,
            "macro": 15,
            "label": 10
          },
          "description": "Priority scores for semantic token types (higher = better jump characters)",
          "additionalProperties": {
            "type": "number"
          },
          "order": 8006
        },
        "vstoys.semantic-jump.densityThresholds": {
          "type": "object",
          "default": {
            "lowToMedium": 15,
            "mediumToHigh": 40
          },
          "description": "Target count thresholds for adaptive mode switching",
          "properties": {
            "lowToMedium": {
              "type": "number",
              "description": "Threshold for switching from single-char to mixed mode"
            },
            "mediumToHigh": {
              "type": "number",
              "description": "Threshold for switching from mixed to progressive mode"
            }
          },
          "order": 8007
        },
        "vstoys.semantic-jump.decorationBackgroundColor": {
          "type": "string",
          "default": "activityErrorBadge.background",
          "description": "Background color for jump character decorations",
          "order": 8008
        },
        "vstoys.semantic-jump.decorationForegroundColor": {
          "type": "string",
          "default": "button.foreground",
          "description": "Foreground color for jump character decorations",
          "order": 8009
        },
        "vstoys.regular-jump.caseSensitive": {
          "type": "boolean",
          "default": false,
          "description": "Case sensitive search for regular jump",
          "order": 8100
        },
        "vstoys.regular-jump.maxMatches": {
          "type": "number",
          "default": 100,
          "description": "Maximum number of matches to show",
          "order": 8101
        },
        "vstoys.regular-jump.minWordLength": {
          "type": "number",
          "default": 0,
          "description": "Minimum word length to match (0 = no limit)",
          "order": 8102
        },
        "vstoys.regular-jump.minPatternLength": {
          "type": "number",
          "default": 2,
          "description": "Minimum pattern length before transitioning to jump mode",
          "order": 8103
        },
        "vstoys.regular-jump.maxMatchesForAutoJump": {
          "type": "number",
          "default": 20,
          "description": "Maximum matches to auto-transition to jump mode",
          "order": 8104
        },
        "vstoys.regular-jump.primaryForegroundColor": {
          "type": "string",
          "default": "button.foreground",
          "description": "Theme color for primary (current) match text color - use hex e.g (#FFFFFF) or theme names (editor.foreground)",
          "order": 8105
        },
        "vstoys.regular-jump.primaryMatchColor": {
          "type": "string",
          "default": "activityErrorBadge.background",
          "description": "Theme color for primary (current) match highlighting - use hex e.g (#FFFFFF) or theme names (editor.foreground)",
          "order": 8106
        },
        "vstoys.regular-jump.secondaryForegroundColor": {
          "type": "string",
          "default": "activityBarBadge.foreground",
          "description": "Theme color for secondary (current) match highlighting - use hex e.g (#FFFFFF) or theme names (editor.foreground)",
          "order": 8107
        },
        "vstoys.regular-jump.secondaryMatchColor": {
          "type": "string",
          "default": "activityBarBadge.background",
          "description": "Theme color for secondary matches highlighting - use hex e.g (#FFFFFF) or theme names (editor.foreground)",
          "order": 8108
        },
        "vstoys.regular-jump.jumpLabelBackgroundColor": {
          "type": "string",
          "default": "activityErrorBadge.background",
          "description": "Theme color for jump character label background - use hex e.g (#FFFFFF) or theme names (editor.foreground)",
          "order": 8109
        },
        "vstoys.regular-jump.jumpLabelForegroundColor": {
          "type": "string",
          "default": "button.foreground",
          "description": "Theme color for jump character label text - use hex e.g (#FFFFFF) or theme names (editor.foreground)",
          "order": 8110
        },
        "vstoys.regular-jump.primaryBorderColor": {
          "type": "string",
          "default": "activityBarBadge.background",
          "description": "Theme color for the border around the primary match, empty string does not draw a border - use hex e.g (#FFFFFF) or theme names (editor.foreground)",
          "order": 8111
        },
        "vstoys.registers.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable the Registers module in VSCode Toys",
          "order": 6000
        },
        "vstoys.registers.maxPreviewLength": {
          "type": "number",
          "default": 200,
          "description": "Maximum preview length in quick pick menu for register contents",
          "order": 6001
        },
        "vstoys.registers.showEmptyRegisters": {
          "type": "boolean",
          "default": true,
          "description": "Show all registers (including empty ones) in quick paste menu. When disabled, only shows registers with content.",
          "order": 6002
        },
        "vstoys.paste-replace.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable the Paste Replace module in VSCode Toys",
          "order": 7000
        },
        "vstoys.paste-replace.reindentBeforePaste": {
          "type": "boolean",
          "default": true,
          "description": "Automatically reindent lines before pasting content when using smart paste",
          "order": 7001
        },
        "vstoys.motions.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable the Motions module in VSCode Toys",
          "order": 3000
        },
        "vstoys.motions.foregroundColor": {
          "type": "string",
          "description": "Foreground color for yank highlighting",
          "order": 3001
        },
        "vstoys.motions.backgroundColor": {
          "type": "string",
          "default": "editor.wordHighlightBackground",
          "description": "Background color for yank highlighting\\nFor more information about color check out https://code.visualstudio.com/api/references/theme-color for more available colors\\nrgba(R, G, B, A) is also supported",
          "order": 3002
        },
        "vstoys.motions.timeout": {
          "type": "number",
          "default": 100,
          "description": "Highlight timeout duration in milliseconds for yank operations",
          "order": 3003
        },
        "vstoys.hyper.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable the Hyper module in VSCode Toys",
          "order": 3000
        },
        "vstoys.hyper.normalLayers": {
          "type": "array",
          "default": [
            {
              "name": "hyper-layer",
              "enabled": true
            }
          ],
          "description": "List of active Hyper layers to register, activates a layer without deactivating others.",
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "description": "The name of the hyper layer"
              },
              "timeout": {
                "type": "number",
                "description": "The timeout for this layer in seconds",
                "default": 6
              },
              "enabled": {
                "type": "boolean",
                "description": "Whether this layer is enabled",
                "default": true
              }
            },
            "required": [
              "name"
            ]
          },
          "order": 3001
        },
        "vstoys.hyper.switchLayers": {
          "type": "array",
          "default": [
            {
              "name": "commit-layer",
              "timeout": 900,
              "enabled": true
            }
          ],
          "description": "List of active Hyper layers to register, deactivates all other layers then activates a layer.",
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "description": "The name of the hyper layer"
              },
              "timeout": {
                "type": "number",
                "description": "The timeout for this layer in seconds",
                "default": 6
              },
              "enabled": {
                "type": "boolean",
                "description": "Whether this layer is enabled",
                "default": true
              }
            },
            "required": [
              "name"
            ]
          },
          "order": 3002
        },
        "vstoys.goto-line.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable the Goto Line module in VSCode Toys",
          "order": 3000
        },
        "vstoys.goto-line.upCharacter": {
          "type": "string",
          "default": "k",
          "description": "Character prefix for moving up in relative goto (e.g., 'k5' to go up 5 lines)",
          "order": 3001
        },
        "vstoys.goto-line.downCharacter": {
          "type": "string",
          "default": "j",
          "description": "Character prefix for moving down in relative goto (e.g., 'j5' to go down 5 lines)",
          "order": 3002
        },
        "vstoys.goto-line.highlightingEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable line highlighting preview when using goto commands",
          "order": 3003
        },
        "vstoys.goto-line.selectColor": {
          "type": "string",
          "default": "editor.wordHighlightBackground",
          "description": "Color for highlighting lines during selection operations (theme color ID or hex color)",
          "order": 3004
        },
        "vstoys.goto-line.deleteColor": {
          "type": "string",
          "default": "editorMarkerNavigationError.headerBackground",
          "description": "Color for highlighting lines during delete operations (theme color ID or hex color)",
          "order": 3005
        },
        "vstoys.goto-line.copyColor": {
          "type": "string",
          "default": "editor.findRangeHighlightBackground",
          "description": "Color for highlighting lines during copy operations (theme color ID or hex color)",
          "order": 3006
        },
        "vstoys.goto-line.cutColor": {
          "type": "string",
          "default": "editorMarkerNavigationWarning.headerBackground",
          "description": "Color for highlighting lines during cut (args: 'delete' and 'copy' operations create cut) (theme color ID or hex color)",
          "order": 3007
        },
        "vstoys.git.fileDecorator.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable the File Decorator module in VSCode Toys",
          "order": 5000
        },
        "vstoys.git.fileDecorator.enableBadge": {
          "type": "boolean",
          "default": true,
          "description": "Enable badge for file decorations",
          "order": 5001
        },
        "vstoys.git.fileDecorator.enableColor": {
          "type": "boolean",
          "default": true,
          "description": "Enable color for file decorations",
          "order": 5002
        },
        "vstoys.git.fileDecorator.refreshInterval": {
          "type": "number",
          "default": 5000,
          "description": "Interval in milliseconds for refreshing file decorations (minimum 1000ms)",
          "order": 5003
        },
        "vstoys.git.fileDecorator.targetBranch": {
          "type": "string",
          "default": "main",
          "description": "Target branch for Git diff comparisons",
          "ignoreSync": true,
          "order": 5004
        },
        "vstoys.git.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable the Git module in VSCode Toys",
          "order": 4000
        },
        "vstoys.copy-highlight.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable the Copy Highlight module in VSCode Toys",
          "order": 2000
        },
        "vstoys.copy-highlight.foregroundColor": {
          "type": "string",
          "description": "Foreground color for the copy highlighting",
          "order": 2001
        },
        "vstoys.copy-highlight.backgroundColor": {
          "type": "string",
          "default": "editor.wordHighlightBackground",
          "description": "Background color for the copy highlighting\nFor more information about color check out https://code.visualstudio.com/api/references/theme-color for more available colors\nrgba(R, G, B, A) is also supported",
          "order": 2002
        },
        "vstoys.copy-highlight.timeout": {
          "type": "number",
          "default": 100,
          "description": "Timeout duration in milliseconds",
          "order": 2003
        },
        "vstoys.clear-line.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable the Clear Line module in VSCode Toys",
          "order": 1000
        }
      }
    },
    "keybindings": [
      {
        "command": "vstoys.smart-open.navigateUp",
        "key": "up",
        "when": "vstoys.smart-open.active && vstoys.smart-open.searching"
      },
      {
        "command": "vstoys.smart-open.navigateDown",
        "key": "down",
        "when": "vstoys.smart-open.active && vstoys.smart-open.searching"
      },
      {
        "command": "vstoys.smart-open.selectFile",
        "key": "enter",
        "when": "vstoys.smart-open.active && vstoys.smart-open.searching"
      },
      {
        "command": "vstoys.smart-open.deleteChar",
        "key": "backspace",
        "when": "vstoys.smart-open.active && vstoys.smart-open.searching"
      },
      {
        "key": "f19",
        "command": "vstoys.regular-jump.start",
        "when": "hyper-active && !vstoys.regular-jump.active && !vstoys.semantic-jump.active"
      },
      {
        "key": "ctrl+f19",
        "command": "vstoys.semantic-jump.jump",
        "when": "!vstoys.regular-jump.active && !vstoys.semantic-jump.active"
      },
      {
        "command": "vstoys.semantic-jump.escape",
        "key": "escape",
        "when": "!vstoys.regular-jump.active && vstoys.semantic-jump.active"
      },
      {
        "command": "vstoys.regular-jump.escape",
        "key": "escape",
        "when": "vstoys.regular-jump.active && !vstoys.semantic-jump.active"
      },
      {
        "command": "vstoys.regular-jump.backspace",
        "key": "backspace",
        "when": "vstoys.regular-jump.active"
      },
      {
        "command": "vstoys.regular-jump.enter",
        "key": "enter",
        "when": "vstoys.regular-jump.active"
      },
      {
        "command": "vstoys.regular-jump.next",
        "key": "f19",
        "when": "vstoys.regular-jump.active"
      },
      {
        "command": "vstoys.regular-jump.previous",
        "key": "ctrl+f19",
        "when": "vstoys.regular-jump.active"
      },
      {
        "command": "vstoys.paste-replace.clipboardPasteReplace",
        "key": "ctrl+shift+v",
        "mac": "cmd+shift+v",
        "when": "editorTextFocus && vstoys.paste-replace.active"
      },
      {
        "command": "vstoys.paste-replace.clipboardPasteSmart",
        "key": "ctrl+v",
        "mac": "cmd+v",
        "when": "editorTextFocus && vstoys.paste-replace.active"
      },
      {
        "command": "editor.action.clipboardPasteAction",
        "key": "ctrl+k ctrl+v",
        "mac": "ctrl+k cmd+v",
        "when": "vstoys.paste-replace.active"
      },
      {
        "command": "vstoys.motions.escape",
        "key": "escape",
        "when": "vstoys.motions.inputActive"
      },
      {
        "command": "vstoys.hyper.layerActivate.hyper-layer",
        "key": "f20",
        "mac": "f20",
        "when": "vstoys.hyper.active"
      },
      {
        "key": "f20",
        "command": "vstoys.hyper.layerDeactivate.hyper-layer",
        "when": "vstoys.hyper.active && vstoys.hyper.global && hyper-layer"
      },
      {
        "command": "vstoys.hyper.deactivateAll",
        "key": "escape",
        "mac": "escape",
        "when": "vstoys.hyper.global"
      },
      {
        "key": "alt+s f",
        "command": "vstoys.git.stageFile",
        "when": "editorTextFocus && vstoys.git.active"
      },
      {
        "key": "alt+s h",
        "command": "vstoys.git.stageHunk",
        "when": "editorTextFocus && vstoys.git.active"
      },
      {
        "command": "vstoys.copy-highlight.copy",
        "key": "ctrl+c",
        "mac": "cmd+c",
        "when": "editorTextFocus && vstoys.copy-highlight.active"
      },
      {
        "key": "d c",
        "command": "vstoys.clear-line.clearLines",
